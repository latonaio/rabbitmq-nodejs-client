{
  "name": "rabbitmq-client",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "amqp-connection-manager": "^3.5.2",
    "amqplib": "^0.8.0"
  },
  "devDependencies": {
    "@types/amqp-connection-manager": "^2.0.12",
    "@types/amqplib": "^0.8.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.2",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
